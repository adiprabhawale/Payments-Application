{
  "name": "unified-payments-banking-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 expo start",
    "android": "cross-env EXPO_NO_TELEMETRY=1 expo start --android",
    "ios": "cross-env EXPO_NO_TELEMETRY=1 expo start --ios",
    "web": "cross-env EXPO_NO_TELEMETRY=1 expo start --web",
    "server": "nodemon server/index.js",
    "dev:android": "concurrently \"npm run server\" \"npm run android\"",
    "dev:ios": "concurrently \"npm run server\" \"npm run ios\"",
    "dev:web": "concurrently \"npm run server\" \"npm run web\"",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:server": "jest server/__tests__"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@lucide/lab": "^0.1.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.3",
    "expo-camera": "~16.1.5",
    "expo-constants": "~17.1.3",
    "expo-font": "~13.2.2",
    "expo-haptics": "~14.1.3",
    "expo-linear-gradient": "~14.1.3",
    "expo-linking": "~7.1.3",
    "expo-router": "~5.0.2",
    "expo-splash-screen": "~0.30.6",
    "expo-status-bar": "~2.2.2",
    "expo-symbols": "~0.4.3",
    "expo-system-ui": "~5.0.5",
    "expo-web-browser": "~14.1.5",
    "express": "^5.1.0",
    "lucide-react-native": "^0.475.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.3.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "~19.0.10",
    "@types/uuid": "^9.0.8",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "jest-expo": "^52.0.2",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "typescript": "~5.8.3"
  }
}
